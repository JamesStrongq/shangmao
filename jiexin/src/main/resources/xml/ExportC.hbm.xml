<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.itcast.jk.domain.ExportC" table="export_c" schema="jkdb">
        <id name="exportId" column="EXPORT_ID"/>
        <property name="packingListId" column="PACKING_LIST_ID"/>
        <property name="contractId" column="CONTRACT_ID"/>
        <property name="inputDate" column="INPUT_DATE"/>
        <property name="contractIds" column="CONTRACT_IDS"/>
        <property name="customerContract" column="CUSTOMER_CONTRACT"/>
