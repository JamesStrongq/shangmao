<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.itcast.jk.domain.FactoryC" table="factory_c" schema="jkdb">
        <id name="factoryId" column="FACTORY_ID" type="java.lang.String"/>
        <property name="fullName" column="FULL_NAME" type="java.lang.String"/>
        <property name="factoryName" column="FACTORY_NAME" type="java.lang.String"/>
        <property name="contractor" column="CONTRACTOR" type="java.lang.String"/>
        <property name="phone" column="PHONE" type="java.lang.String"/>
        <property name="mobile" column="MOBILE" type="java.lang.String"/>
        <property name="fax" column="FAX" type="java.lang.String"/>
        <property name="note" column="NOTE" type="java.lang.String"/>
        <property name="orderNo" column="ORDER_NO" type="java.lang.Integer"/>
        <property name="state" column="STATE" type="java.lang.Integer"/>
    </class>
</hibernate-mapping>